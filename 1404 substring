#include <stdio.h>

void main()
{
  int i,flag=0;
  char str[5],sub[5];
  printf("Enter the String");
  scanf("%s",str);
  printf("Enter the Substring");
  scanf("%s",sub);
  for(i=0;i<5;i++)
  if(sub[i]==str[i])
  {
    flag=1;
   }
   if(flag==1)
   printf("Substring present");
   else
   printf("Substring not present");
}
