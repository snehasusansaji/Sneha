#include <stdio.h>
void main()
{
  int i,num,flag=0;
  printf("Enter the number");
  scanf("%d",&num);
 prime();
 if(flag==0)
   printf("Prime number");
 else
   printf("Not a Prime number");
}
int prime(int num,int flag)
{
  int i;
  for(i=2;i<=num-1;i++)
  {
    if(num%i==0)
    {
      flag=1;
      return flag;
      break;
    }
  }
}


